package Practice;

public class Student {
	
	String name;
	int grade;
	String department;
	
	//1
	Student(){}
	//2
	Student(String n){
		
	}
	//3
	Student(String n, int g){
		name = n;
		grade = g;
		
	}
	//4
	Student(String n, int g, String d){
		name = n;
		grade = g;
		department = d;
	}
	
	//5
	Student(String d, int g){
		department = d;
		grade = g;
	}


}
