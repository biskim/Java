package ch04;

public class OpEx3 {
	
	public static void main(String[] args) {
		
		int c = 10;
		int d = 10;
		
		++c;
		d++;
		
		System.out.println(c);
		System.out.println(d);
		
		int a = 10;
		int b = ++a;
		
		System.out.println("전위연산 결과 : "+b);
		
		int x = 10;
		int y = x++;
		
		System.out.println("후위연산 결과 : "+y);
		System.out.println("x : "+x);
	}

}
