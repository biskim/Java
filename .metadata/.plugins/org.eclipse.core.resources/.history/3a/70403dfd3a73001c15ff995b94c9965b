public class Sub3_SingletonTest {
	
    public static void main(String[] args) {
    	
    	Calc c = Calc.getInstance();
    	
    	Calc c = new Calc();
    	
    	int r1 = c.plus(1,  2);
    	int r2 = c.minus(1,  2);
    	int r3 = c.multi(1,  2);
    	int r4 = c.div(1,  2);
    	
    	System.out.println("r1 : "+r1);
    	System.out.println("r2 : "+r2);
    	System.out.println("r3 : "+r3);
    	System.out.println("r4 : "+r4);
		
	}

}
