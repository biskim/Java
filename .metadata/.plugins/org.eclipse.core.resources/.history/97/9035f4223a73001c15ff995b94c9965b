package ch04;
/*
 * 날짜 : 2022/01/03
 * 이름 : 김병현
 * 내용 : 산술연산 +,-연산
 */

public class p55 {
	public static void main(String[] args) {
		int a = 5;
		
		System.out.println("+a = " + +a);
		System.out.println("-a = " + -a);
		
		a = -5;
		
		System.out.println("+a = " + +a);
		System.out.println("+a = " + -a);
	}

}
