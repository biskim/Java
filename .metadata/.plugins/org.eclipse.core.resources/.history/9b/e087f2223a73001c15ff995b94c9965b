package ch04;
/*
 * 날짜 : 2022/01/03
 * 이름 : 김병현
 * 내용 : 대입연산 사칙연산 + "%"
 */

public class P53 {
	public static void main(String[] args) {
		
		//대입연산
		
		int number1 = 10;
		System.out.println("number1 = 10 -> "+number1);
		number1 += 10;
		System.out.println("number1 += 10 -> "+number1);
		number1 -= 10;
		System.out.println("number1 -= 10 -> "+number1);
		number1 *= 2;
		System.out.println("number1 *= 10 -> "+number1);
		number1 /= 2;
		System.out.println("number1 /= 10 -> "+number1);
		number1 %= 3;
		System.out.println("number1 %= 3 -> "+number1);
	}

}
