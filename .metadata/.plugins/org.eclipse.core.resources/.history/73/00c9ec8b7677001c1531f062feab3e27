package ch02;

import java.util.Scanner;

public class Ex2884 {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int H = sc.nextInt();
		int M = sc.nextInt();
		
		sc.close();
		
		if (M>=45) {
		}else if (M<45) {
			System.out.println(H+""+(M-45+60));
		}else if (H==0) {
			System.out.println();
			
		}
		
		
	
	}System.out.print((H-1) + (M-45))l

}
