public class p54 {
	public static void main(String[] args) {
		int a = 5 + 5; 
				System.out.println("5 + 5 = "+a);
		int b = a - 5;
		System.out.println("10 - 5 = "+b);
		int c = b * 2;
		System.out.println("5 * 2 = " +c);
		int d = c / 5; 
		System.out.println("10 / 5 = " +d);
		
	}
	}


