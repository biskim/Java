package ch2;

import java.util.Scanner;

public class Ex1330 {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		//A, B = map(int, sc.next());
		
		int A = sc.nextInt();
		int B = sc.nextInt();
		
		if(A > B) {
			System.out.println(">");
		}else if(A < B ) {
			System.out.println("<");
		}else if(A == B) {
			System.out.println("==");
		}
		
		sc.close();
		
		
		
	}

}
