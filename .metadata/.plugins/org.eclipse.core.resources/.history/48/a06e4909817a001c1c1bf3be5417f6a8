package ch03;

import java.util.Scanner;

public class Ex2741 {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		
		sc.close();
		
		int k = 0;
		
		for (int i =1; i <= n; i++) {
			
			System.out.print(i);
		}
		System.out.println();
	}

}
